#!/bin/bash
#usage: ls | xargs -I '{}' git-st-one '{}'

projPath=$(pwd)/$1
cd $projPath
isGitProj=$(ls -a | grep '^\.git$')
if [ "$isGitProj" == ".git" ]; then
	status=$(git status)
	status=$(echo $status)
	if [ "$status" != "# On branch master nothing to commit (working directory clean)" ]; then
		echo $1
		git st
	fi
fi
